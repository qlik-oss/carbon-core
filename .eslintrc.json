{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", {}, {
            "usePrettierrc": false
          }],
          "no-unused-vars":["error", {"argsIgnorePattern": "^_"}]
    }
}
